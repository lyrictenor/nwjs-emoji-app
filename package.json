    "redux": "^1.0.0-alpha"
  },
  "devDependencies": {
    "babel": "^5.6.14",
    "babel-core": "^5.6.15",
    "babel-eslint": "^3.1.23",
    "babel-loader": "^5.3.1",
    "cheerio": "^0.19.0",
    "concat-stream": "^1.5.0",
    "cpx": "^1.2.0",
    "css-loader": "^0.15.1",
    "eslint": "^0.24.0",
    "eslint-loader": "^0.14.1",
    "eslint-plugin-react": "^2.6.4",
    "extract-text-webpack-plugin": "^0.8.2",
    "html-webpack-plugin": "^1.5.2",
    "json-loader": "^0.5.2",
    "less": "^2.5.1",
    "less-loader": "^2.2.0",
    "node-libs-browser": "^0.5.2",
    "posix-cat": "^1.1.1",
    "react-hot-loader": "^1.2.7",
    "rimraf": "^2.4.1",
    "style-loader": "^0.12.3",
