  "homepage": "https://github.com/lyrictenor/nwjs-emoji-app",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lyrictenor/nwjs-emoji-app.git"
  },
  "scripts": {
    "build": "npm run clean && npm run resource && webpack",
    "clean": "rimraf dist",
    "dev": "npm run build -- --config webpack.config.dev.js",
    "dist": "npm run build -- --config webpack.config.production.js",
    "lint": "eslint --ext .js,.jsx lib bin",
    "local": "npm run build",
    "parse": "cat ./emoji-cheat-sheet.com/public/index.html | babel-node bin/parse-emoji-cheat-sheet.js > ./emoji-cheat-sheet-data.json",
    "resource": "cpx 'assets/images/**/.[!.]*' dist/assets/images && cpx 'emoji-cheat-sheet.com/public/graphics/emojis/*.png' dist/assets/images/emojis",
    "start": "npm run local && node server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "verify": "npm run local && npm run lint"
  }
}
