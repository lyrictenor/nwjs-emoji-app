{
  "name": "emoji-app",
  "version": "0.1.0",
  "author": "Sanemat <ogataken@gmail.com>",
  "bugs": {
    "url": "https://github.com/lyrictenor/nwjs-emoji-app/issues"
  },
  "dependencies": {
    "babel-runtime": "^5.6.15",
    "capitalize": "^1.0.0",
    "classnames": "^2.1.3",
    "react": "^0.13.3",
    "redux": "^1.0.0-alpha"
  },
  "devDependencies": {
    "babel": "^5.6.14",
    "babel-core": "^5.6.15",
    "babel-eslint": "^3.1.23",
    "babel-loader": "^5.3.1",
    "babel-plugin-espower": "^1.0.0",
    "cheerio": "^0.19.0",
    "concat-stream": "^1.5.0",
    "conventional-changelog": "^0.1.0-alpha.3",
    "cpx": "^1.2.0",
    "css-loader": "^0.15.1",
    "eslint": "^0.24.0",
    "eslint-loader": "^0.14.1",
    "eslint-plugin-react": "^2.6.4",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "html-webpack-plugin": "^1.5.2",
    "json-loader": "^0.5.2",
    "less": "^2.5.1",
    "less-loader": "^2.2.0",
    "mocha": "^2.2.5",
    "node-libs-browser": "^0.5.2",
    "posix-cat": "^1.1.1",
    "power-assert": "^0.11.0",
    "react-hot-loader": "^1.2.7",
    "rimraf": "^2.4.1",
    "sprity": "^1.0.6",
    "sprity-cli": "^1.0.1",
    "style-loader": "^0.12.3",
    "touch": "0.0.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.10.1",
    "webpack-dev-server": "^1.10.1"
  },
  "homepage": "https://github.com/lyrictenor/nwjs-emoji-app",
  "license": "MIT",
  "main": "index.html",
  "repository": {
    "type": "git",
    "url": "https://github.com/lyrictenor/nwjs-emoji-app.git"
  },
  "scripts": {
    "spriteclean": "rimraf ./assets/images/sprite.png ./assets/images/sprite@2x.png ./assets/styles/sprite.css",
    "spritehotfix": "rimraf ./emoji-cheat-sheet.com/public/graphics/emojis/+1.png ./emoji-cheat-sheet.com/public/graphics/emojis/-1.png",
    "sprite": "npm run spritehotfix && npm run spriteclean && sprity create ./assets/images/ ./emoji-cheat-sheet.com/public/graphics/emojis/*.png -s ../styles/sprite.css --prefix sprite -d 1:72 -d 2:192",
    "build": "npm run clean && npm run resource && webpack",
    "changelog": "node ./node_modules/touch/bin/touch.js CHANGELOG.md && conventional-changelog -i CHANGELOG.md --overwrite --preset angular",
    "clean": "rimraf dist",
    "dev": "npm run build -- --config webpack.config.dev.js",
    "dist": "npm run build -- --config webpack.config.production.js",
    "lint": "eslint --ext .js,.jsx lib bin",
    "local": "npm run build",
    "parse": "cat ./emoji-cheat-sheet.com/public/index.html | babel-node bin/parse-emoji-cheat-sheet.js > ./emoji-cheat-sheet-data.json",
    "resource": "npm run sprite && cpx 'assets/images/**/.[!.]*' dist/assets/images && cpx './package.json' dist",
    "start": "npm run local && node server.js",
    "test": "mocha --require ./hook_for_power_assert.js -- test/**/*.js",
    "verify": "npm run local && npm run lint && npm run test && npm run dev && npm run dist"
  },
  "window": {
    "width": 800
  }
}
